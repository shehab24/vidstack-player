{
	"apiVersion": "1",
	"name": "vidstack/media-player",
	"title": "Vidstack Media Player",
	"description": "Description of Block name.",
	"category": "common",
	"keywords": [
		"key1",
		"key2",
		"key3"
	],
	"textdomain": "vsdp",
	"attributes": {
		"align": {
			"type": "string",
			"default": ""
		},
		"cId": {
			"type": "string",
			"default": ""
		},
		"tabs": {
			"type": "string",
			"default": "video"
		},
		"video": {
			"type": "object",
			"default": {
				"url": "https://stream.mux.com/VZtzUzGRv02OhRnZCxcNg49OilvolTqdnFLEqBsTwaxU/low.mp4",
				"title": "Vidstack Media Player"
			}
		},
		"audio": {
			"type": "object",
			"default": {
				"url": "https://media-files.vidstack.io/sprite-fight/audio.mp3",
				"title": "Vidstack Media Player"
			}
		},
		"videoTitle": {
			"type": "string",
			"default": "Vidstack Media Player"
		},
		"audioTitle": {
			"type": "string",
			"default": "Vidstack Media Player"
		},
		"posterUrl": {
			"type": "object",
			"default": {
				"url": "https://image.mux.com/VZtzUzGRv02OhRnZCxcNg49OilvolTqdnFLEqBsTwaxU/thumbnail.webp"
			}
		},
		"audioPosterUrl": {
			"type": "object",
			"default": {
				"url": "https://image.mux.com/VZtzUzGRv02OhRnZCxcNg49OilvolTqdnFLEqBsTwaxU/thumbnail.webp"
			}
		},
		"isCaption": {
			"type": "boolean",
			"default": true
		},
		"captionUrl": {
			"type": "string",
			"default": "https://media-files.vidstack.io/sprite-fight/subs/english.vtt"
		},
		"chapterUrl": {
			"type": "string",
			"default": "https://media-files.vidstack.io/sprite-fight/chapters.vtt"
		},
		"buildedUrl": {
			"type": "string",
			"default": ""
		},
		"isChapter": {
			"type": "boolean",
			"default": false
		},
		"chapterSource": {
			"type": "string",
			"default": "file"
		},
		"chapterItems": {
			"type": "array",
			"default": [
				{
					"start": "00:00",
					"end": "01:00",
					"chapterText": "Chapter 1"
				}
			]
		},
		"captionItems": {
			"type": "array",
			"default": [
				{
					"captionUrl": "https://media-files.vidstack.io/sprite-fight/subs/english.vtt",
					"label": "English"
				}
			]
		},
		"items": {
			"type": "array",
			"default": [
				{
					"number": 10,
					"text": "Vertical"
				}
			]
		},
		"columns": {
			"type": "object",
			"default": {
				"desktop": 3,
				"tablet": 2,
				"mobile": 1
			}
		},
		"columnGap": {
			"type": "string",
			"default": "30px"
		},
		"rowGap": {
			"type": "string",
			"default": "40px"
		},
		"layout": {
			"type": "string",
			"default": "vertical"
		},
		"alignment": {
			"type": "string",
			"default": "center"
		},
		"textAlign": {
			"type": "string",
			"default": "center"
		},
		"width": {
			"type": "string",
			"default": "80%"
		},
		"background": {
			"type": "object",
			"default": {
				"color": "#0000"
			}
		},
		"content": {
			"type": "string",
			"default": "Content of the block",
			"selector": ".prefixBlockName .content"
		},
		"typography": {
			"type": "object",
			"default": {
				"fontSize": 25
			}
		},
		"color": {
			"type": "string",
			"default": "#333"
		},
		"captionColor": {
			"type": "string",
			"default": "#fff"
		},
		"captionBgColor": {
			"type": "string"
		},
		"chapterColor": {
			"type": "string",
			"default": "#fff"
		},
		"chapterBgColor": {
			"type": "string"
		},
		"settingsColor": {
			"type": "string",
			"default": "#fff"
		},
		"settingsBgColor": {
			"type": "string"
		},
		"volumeColor": {
			"type": "string",
			"default": "#fff"
		},
		"volumeBgColor": {
			"type": "string"
		},
		"mediaTimeColor": {
			"type": "string",
			"default": "#fff"
		},
		"playbtnColor": {
			"type": "string",
			"default": "#fff"
		},
		"playbtnBgColor": {
			"type": "string"
		},
		"pipbtnColor": {
			"type": "string",
			"default": "#fff"
		},
		"pipbtnBgColor": {
			"type": "string"
		},
		"screenbtnColor": {
			"type": "string",
			"default": "#fff"
		},
		"screenbtnBgColor": {
			"type": "string"
		},
		"mediaTitleColor": {
			"type": "string",
			"default": "#fff"
		},
		"globalSliderColor": {
			"type": "string",
			"default": "#fff"
		},
		"globalPrimaryColor": {
			"type": "string"
		},
		"globalSecondaryColor": {
			"type": "string",
			"default": "#fff"
		},
		"colors": {
			"type": "object",
			"default": {
				"color": "#333",
				"bg": "#fff"
			}
		},
		"img": {
			"type": "object",
			"default": {
				"id": null,
				"url": "",
				"alt": "",
				"title": ""
			}
		},
		"isIcon": {
			"type": "boolean",
			"default": true
		},
		"icon": {
			"type": "object",
			"default": {
				"class": "fab fa-wordpress"
			}
		},
		"separator": {
			"type": "object",
			"default": {
				"width": "20%",
				"height": "2px",
				"style": "solid",
				"color": "#bbb"
			}
		},
		"padding": {
			"type": "object",
			"default": {
				"vertical": "15px",
				"horizontal": "30px"
			}
		},
		"margin": {
			"type": "object",
			"default": {
				"bottom": "15px"
			}
		},
		"border": {
			"type": "object",
			"default": {
				"radius": "5px"
			}
		},
		"shadow": {
			"type": "array",
			"default": {}
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true,
			"columns": {
				"desktop": 1,
				"tablet": 1,
				"mobile": 1
			}
		}
	},
	"editorScript": "file:../dist/editor.js"
}